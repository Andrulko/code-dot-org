{
  "presets": [
    "react",
    [
      "es2015",
      {
        "modules": false,
      }
    ],
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    "transform-class-properties",
    // needed for IE 9/10 support
    ["transform-es2015-classes", {"loose": true}]
  ]
}
